{
  "expo": {
    "name": "My Expo App",
    "slug": "testyt-dlp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#f0f8ff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.testytdlp.app",
      "infoPlist": {
        "UIBackgroundModes": ["audio"],
        "NSAppleMusicUsageDescription": "This app needs access to media library to display song information in control center",
        "AVAudioSessionCategory": "AVAudioSessionCategoryPlayback",
        "AVAudioSessionCategoryOptions": ["AVAudioSessionCategoryOptionAllowBluetooth", "AVAudioSessionCategoryOptionAllowBluetoothA2DP"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#f0f8ff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.testytdlp.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "platforms": ["ios", "android", "web"]
  }
}
